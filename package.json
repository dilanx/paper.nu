{
  "name": "paper.nu",
  "version": "3.1.0",
  "private": true,
  "homepage": "https://www.paper.nu",
  "dependencies": {
    "@dilanx/avatar": "^1.0.8",
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "debug": "^4.3.4",
    "framer-motion": "^7.3.5",
    "html-to-image": "^1.10.8",
    "ics": "^2.40.0",
    "leaflet": "^1.9.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-leaflet": "^4.1.0",
    "react-select": "^5.8.0",
    "spinners-react": "^1.0.7"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@dilanx/config": "^1.1.0",
    "@types/debug": "^4.1.7",
    "@types/leaflet": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.16",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false REACT_APP_COMMIT=\"$(git rev-parse --short HEAD)\" BROWSER=none craco start",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_COMMIT=\"$(git rev-parse --short HEAD)\" craco build",
    "postbuild": "bash -c \"$(curl -fsSL dilanxd.com/preperror.sh)\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --list-different src",
    "predeploy:beta": "npm run build",
    "deploy:beta": "gh-pages -d build -b beta-build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
